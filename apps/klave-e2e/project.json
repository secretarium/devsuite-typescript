{
    "name": "klave-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/klave-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nx/cypress:cypress",
            "options": {
                "cypressConfig": "apps/klave-e2e/cypress.config.ts",
                "devServerTarget": "klave:serve:development",
                "testingType": "e2e"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "klave:serve:production"
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "apps/klave-e2e/**/*.{js,ts,json}"
                ]
            }
        }
    },
    "tags": [],
    "implicitDependencies": [
        "klave"
    ]
}
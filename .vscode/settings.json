{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "editor.tabSize": 4,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.detectIndentation": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#b2294c",
        "titleBar.activeForeground": "#f8ece6",
        "titleBar.inactiveBackground": "#7e1d36",
        "titleBar.inactiveForeground": "#e0c4bd"
    },
    "typescript.tsdk": "node_modules\\typescript\\lib"
}
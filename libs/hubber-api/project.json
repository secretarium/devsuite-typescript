{
    "name": "hubber-api",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/hubber-api/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:swc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/libs/hubber-api",
                "main": "libs/hubber-api/src/index.ts",
                "tsConfig": "libs/hubber-api/tsconfig.lib.json",
                "assets": [
                    "libs/hubber-api/*.md"
                ]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/hubber-api/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/hubber-api/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
{
    "name": "hubber",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/hubber/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/node:webpack",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/packages/hubber",
                "main": "packages/hubber/src/main.ts",
                "tsConfig": "packages/hubber/tsconfig.app.json",
                "assets": [
                    "packages/hubber/src/assets"
                ]
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "extractLicenses": true,
                    "inspect": false,
                    "fileReplacements": [
                        {
                            "replace": "packages/hubber/src/environments/environment.ts",
                            "with": "packages/hubber/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve": {
            "executor": "@nrwl/node:node",
            "options": {
                "buildTarget": "hubber:build"
            },
            "configurations": {
                "production": {
                    "buildTarget": "hubber:build:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/hubber/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/packages/hubber"
            ],
            "options": {
                "jestConfig": "packages/hubber/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "version": {
            "executor": "@jscutlery/semver:version",
            "options": {
                "preset": "conventional",
                "trackDeps": true
            }
        }
    },
    "tags": []
}